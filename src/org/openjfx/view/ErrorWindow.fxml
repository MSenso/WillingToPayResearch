<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="inputPane" prefHeight="400.0" prefWidth="543.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.controller.ErrorWindow">
    <center>
        <GridPane alignment="CENTER" prefHeight="400.0" prefWidth="500.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="110.0" minWidth="0.0" prefWidth="0.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="526.0" minWidth="10.0" prefWidth="526.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="127.0" minWidth="0.0" prefWidth="0.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="64.0" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="155.0" minHeight="10.0" prefHeight="47.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="352.0" minHeight="10.0" prefHeight="300.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="100.0" minHeight="0.0" prefHeight="45.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label fx:id="errorTitle" alignment="CENTER" contentDisplay="CENTER" prefHeight="58.0" prefWidth="400.0" text="Допущена ошибка:" textAlignment="CENTER" textFill="#687aab" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1">
                    <font>
                        <Font name="System Bold" size="16.0" />
                    </font>
                </Label>
                <ListView fx:id="testError" prefHeight="319.0" prefWidth="506.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <Button fx:id="okButton" mnemonicParsing="false" onAction="#handleOkButtonAction" prefHeight="39.0" prefWidth="101.0" style="-fx-background-color: #000000,        linear-gradient(#7ebcea, #2f4b8f),        linear-gradient(#426ab7, #263e75),        linear-gradient(#687aab, #223768); -fx-background-insets: 0,1,2,3; -fx-background-radius: 3,2,2,2; -fx-text-fill: white; -fx-font-size: 14px; -fx-font-weight: bold;" text="ОК" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
            </children>
        </GridPane>
    </center>
</BorderPane>
